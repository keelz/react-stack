{
  "extends": ["tslint-config-airbnb"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "src/assets/*",
      "src/registerServiceWorker.ts"
    ]
  },
  "rules": {
    "import-name": false,
    "strict-boolean-expressions": false,
    "trailing-comma": false,
    "variable-name": false
  }
}
